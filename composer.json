{
    "name": "satupersen/cache",
    "description": "The Illuminate Cache package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "satupersen/contracts": "^1.0",
        "satupersen/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Satupersen\\Cache\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "ext-memcached": "Required to use the memcache cache driver.",
        "satupersen/database": "Required to use the database cache driver (^1.0).",
        "satupersen/filesystem": "Required to use the file cache driver (^1.0).",
        "satupersen/redis": "Required to use the redis cache driver (^1.0).",
        "symfony/cache": "Required to PSR-6 cache bridge (^5.1)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
